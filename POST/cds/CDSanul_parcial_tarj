		post:PresNro == giros:PresNro_canc && \

	DPRINT "[DBG] AnulTarj: sel pre=[%s] fec=[%s] ope=[%s]\n" giros:PresNro_canc common:system_date op:op_operator_id
	DPRINT "[DBG] AnulTarj: find rv=[%d]\n" l_retval
			post:codigo_interno \
			post:PresNro \
			tlr:jnl_sequence

		DPRINT "[DBG] AnulTarj: jnl pre=[%s] seq=[%s] codint=[%s]\n" post:PresNro tlr:jnl_sequence post:codigo_interno
			DPRINT "[DBG] AnulTarj: mp codseq=[%s] lote=[%s] ext=[%s]\n" post:adt_mp_codseq_nro post:Nro_Lote_Pos post:jnl_opmp_ext_ref
*   OFB:post:CDSanul_parcial_tarj
* DESCRIPCION
*   Consulta en el presupuesto si existe un medio de pago tarjeta 
* ENTRADAS
*   Parámetros de entrada de la rutina, sean argumentos o campos que tiene en cuenta
* SALIDAS
*   Campos modificados como resultado o returns cuyos valores sean no triviales
* IMPACTOS
*   Otros objetos que deberán ser analizados al modificar el objeto actual
* REFERENCIADO POR
*   post:SVALanulacion
* DOCUMENTACION
*   NI50525 POS Integrado
* SOURCE
*/
#include "postcds.h"

SUB post:CDSanul_parcial_tarj
	LOCAL l_retval LIKE lib:err_retval

	CDS SELECT FROM DSC:tlr:TABjnl BECOMES DSC:tlr:jnl_context WHERE \
		tlr:jnl_bus_date == common:system_date && \
		siaf:codigo_siaf == 99099 && \
		post:PresNro == post:PresNro && \
		siaf:moneda == "06" && \
		tlr:jnl_operator == op:op_operator_id
	
	CDS FINDFIRST DSC:tlr:jnl_context NOLOCK
	l_retval = CDSRETVAL
	IF (l_retval == 0) THEN
		CDS EXTRACT DSC:tlr:jnl_context \
			post:codigo_interno
		
		IF (post:codigo_interno == 130410) THEN
			CDS EXTRACT DSC:tlr:jnl_context \
				post:Nro_Lote_Pos \
				post:adt_vpi_codseq_nro INTO post:adt_mp_codseq_nro \
				post:jnl_opmp_ext_ref
		ELSE
			CDS EXTRACT DSC:tlr:jnl_context \
				post:Nro_Lote_Pos \
				post:adt_vpi_codseq_nro \
				post:jnl_opvpi_issuerCode
		END IF
	END IF
	CDS ENDSELECT DSC:tlr:jnl_context 
	
	RETURN l_retval
	
END SUB 

/*******/
